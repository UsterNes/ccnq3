#!/usr/bin/make -f
%:
	dh $@ --parallel

override_dh_gencontrol:
	dh_gencontrol -- \
		-Vfreeswitch:Version="1.3.4-20130930T160940Z-git8efc1d0~shimaore-master" \
		-Vopensips:Version="1.11.0~shimaore-2" \
		-Vmediaproxy:Version="2.5.2-5~shimaore" \
		-Vnodejs:Version="0.10.22~shimaore1-1"

override_dh_installinit:
	dh_installinit -R
