#!/usr/bin/env coffee
fs = require 'fs'
_ = JSON.parse fs.readFileSync process.argv[2], 'utf8'
try
  value = eval "_.#{process.argv[3]}"
catch error
  value = undefined
if typeof value is 'string'
  # Do not quote strings so that we can actually use them in shell scripts.
  console.log value
else
  console.dir value
