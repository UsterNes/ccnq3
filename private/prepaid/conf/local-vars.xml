<!-- Common variables -->
<X-PRE-PROCESS cmd="set" data="prepaid_uri=http://127.0.0.1:5984/prepaid"/>
<X-PRE-PROCESS cmd="set" data="cnam_uri=http://127.0.0.1:5984/callingname.yaws?Number="/>

<X-PRE-PROCESS cmd="set" data="prepaid_profile_name=prepaid"/>
<X-PRE-PROCESS cmd="set" data="prepaid_profile_type=usa-cnam"/>
<X-PRE-PROCESS cmd="set" data="prepaid_egress_target="/><!-- DNS name to send egress calls to -->
<X-PRE-PROCESS cmd="set" data="prepaid_ingress_target="/><!-- DNS name to send ingress calls to -->

<X-PRE-PROCESS cmd="set" data="rtp_ip=auto"/>
<!-- <X-PRE-PROCESS cmd="set" data="ingress_sip_ip=auto"/> --><!-- This will only bind to IPv4 -->
<X-PRE-PROCESS cmd="set" data="ingress_sip_ip=::"/>
<X-PRE-PROCESS cmd="set" data="ingress_sip_port=5200"/>
<!-- <X-PRE-PROCESS cmd="set" data="egress_sip_ip=auto"/> --><!-- This will only bind to IPv4 -->
<X-PRE-PROCESS cmd="set" data="egress_sip_ip=::"/>
<X-PRE-PROCESS cmd="set" data="egress_sip_port=1$${ingress_sip_port}"/>
